{
  "manifest_version": 2,
  "name": "Profile Manager",
  "version": "1",
  "icons": {
    "16": "./favicon.ico"
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "19": "./favicon.ico"
    }
  },
  "permissions": ["tabs", "storage"],
  "content_scripts": [
    {
      "matches": ["https://play.dev.grid.tf/*"],
      "js": ["$$cmds.js", "bus.js"]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
